        <dependency id="Microsoft.OData.Core" version="7.6.4" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="10.0.2" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
