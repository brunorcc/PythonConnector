// *** WriteClientLibrary

public void WriteClientLibrary(ModelWriter modelWriter, ServiceModel model)
{
    if (model == null)
    {
        return;
    }

    if (!model.GenerateClientLibrary)
    {
        this.WriteNoClientLibDocumentation();
        return;
    }

    this.WriteClientLibraryDocumentation(modelWriter, model);
}

